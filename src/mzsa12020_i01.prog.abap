MODULE user_command_0100 INPUT.

  CASE sy-ucomm.
    When 'EXIT'.
      leave PROGRAM.
    WHEN 'BACK' or 'CANC'.
      LEAVE TO SCREEN 0.

  ENDCASE.
ENDMODULE.
